# Slack Insights Environment Configuration
# Copy this file to .env and fill in your values
# NEVER commit the .env file to version control

# Anthropic API Configuration
# Get your API key from: https://console.anthropic.com/account/keys
ANTHROPIC_API_KEY=your_api_key_here

# Database Configuration (optional - defaults to ./slack_insights.db)
# DATABASE_PATH=./slack_insights.db

# AI Model Configuration (optional - uses defaults from tech-stack.md)
# CLAUDE_MODEL=claude-sonnet-4-20250514
# MAX_TOKENS=4096
# TEMPERATURE=0.5

# Batch Processing Configuration (optional)
# BATCH_SIZE=50
# MAX_CONCURRENT_REQUESTS=5

# Logging Configuration (optional)
# LOG_LEVEL=INFO
# LOG_FILE=./slack_insights.log

# Query Cache Configuration (optional)
# ENABLE_QUERY_CACHE=true
# CACHE_TTL_HOURS=24
